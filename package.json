{
  "name": "personal-website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "22.14.0"
  },
  "scripts": {
    "start": "cross-env IS_LOCAL=true NODE_ENV=${NODE_ENV:-development} PUBLIC_APP_ENV=${PUBLIC_APP_ENV:-dev} vike dev",
    "start:dev": "PUBLIC_APP_ENV=dev yarn start",
    "start:staging": "PUBLIC_APP_ENV=staging yarn start",
    "start:prod": "PUBLIC_APP_ENV=prod yarn start",

    "build": "PUBLIC_APP_ENV=${PUBLIC_APP_ENV:-dev} vike build",
    "build:dev": "PUBLIC_APP_ENV=dev yarn build",
    "build:staging": "PUBLIC_APP_ENV=staging yarn build",
    "build:prod": "PUBLIC_APP_ENV=prod yarn build",

    "deploy": "PUBLIC_APP_ENV=${PUBLIC_APP_ENV:-dev} tsx scripts/deploy.cts",
    "deploy:dev": "PUBLIC_APP_ENV=dev yarn deploy",
    "deploy:staging": "PUBLIC_APP_ENV=staging yarn deploy",
    "deploy:prod": "PUBLIC_APP_ENV=prod yarn deploy",

    "create-statsig-secrets": "tsx scripts/create-statsig-secrets.ts",

    "lint": "eslint .",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.777.0",
    "@statsig/react-bindings": "^3.15.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "statsig-node": "^6.3.1",
    "vike": "^0.4.227",
    "vike-react": "^0.6.1",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudformation": "^3.777.0",
    "@aws-sdk/client-cloudfront": "^3.777.0",
    "@aws-sdk/client-s3": "^3.777.0",
    "@eslint/js": "^9.23.0",
    "@jest/globals": "^29.7.0",
    "@statsig/js-client": "^3.15.1",
    "@stylistic/eslint-plugin-ts": "^4.2.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/aws-lambda": "^8.10.148",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.14",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.8.1",
    "aws-lambda": "^1.0.7",
    "cross-env": "^7.0.3",
    "eslint": "^9.23.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "29.3.0",
    "tsx": "^4.19.3",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.28.0",
    "vite": "^6.2.3"
  },
  "packageManager": "yarn@4.8.1"
}
