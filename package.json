{
  "name": "personal-website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "22.14.0"
  },
  "scripts": {
    "start": "cross-env IS_LOCAL=true NODE_ENV=${NODE_ENV:-development} PUBLIC_ENV__APP_ENV=${PUBLIC_ENV__APP_ENV:-dev} vike dev",
    "start:dev": "PUBLIC_ENV__APP_ENV=dev yarn start",
    "start:staging": "PUBLIC_ENV__APP_ENV=staging yarn start",
    "start:prod": "PUBLIC_ENV__APP_ENV=prod yarn start",
    "build": "PUBLIC_ENV__APP_ENV=${PUBLIC_ENV__APP_ENV:-dev} vike build",
    "build:dev": "PUBLIC_ENV__APP_ENV=dev yarn build",
    "build:staging": "PUBLIC_ENV__APP_ENV=staging yarn build",
    "build:prod": "PUBLIC_ENV__APP_ENV=prod yarn build",
    "deploy": "PUBLIC_ENV__APP_ENV=${PUBLIC_ENV__APP_ENV:-dev} tsx scripts/deploy.cts",
    "deploy:dev": "PUBLIC_ENV__APP_ENV=dev yarn deploy",
    "deploy:staging": "PUBLIC_ENV__APP_ENV=staging yarn deploy",
    "deploy:prod": "PUBLIC_ENV__APP_ENV=prod yarn deploy",
    "create-statsig-secrets": "tsx scripts/create-statsig-secrets.ts",
    "lint": "eslint . --fix --cache",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.787.0",
    "@statsig/react-bindings": "^3.15.4",
    "dotenv": "^16.5.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "statsig-node": "^6.3.2",
    "vike": "^0.4.228",
    "vike-react": "^0.6.1",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudformation": "^3.787.0",
    "@aws-sdk/client-cloudfront": "^3.787.0",
    "@aws-sdk/client-s3": "^3.787.0",
    "@eslint/js": "^9.25.1",
    "@statsig/js-client": "^3.15.4",
    "@stylistic/eslint-plugin-ts": "^4.2.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/aws-lambda": "^8.10.149",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "@vitest/ui": "^3.1.2",
    "aws-lambda": "^1.0.7",
    "cross-env": "^7.0.3",
    "eslint": "^9.25.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "sass": "^1.87.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.31.0",
    "vite": "^6.3.2",
    "vitest": "^3.1.2"
  },
  "packageManager": "yarn@4.8.1"
}
